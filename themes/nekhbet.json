{
    "name": "Nekhbet",
    "type": "dark",
    "colors": {
        "editor.background": "#1B1B1B",
        "editor.foreground": "#f5f5f5",
        "editor.lineHighlightBackground": "#313131"
    },
    "tokenColors": [
        {
            "name": "Keyword",
            "scope": [
                "keyword", "storage.type.class"
            ],
            "settings": {
                "foreground": "#f62c7e"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator.assignment",
                "keyword.operator.arithematic"
            ],
            "settings": {
                "foreground": "#00ea00"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function.",
                "support.class.builtin",
                "support.function"
            ],
            "settings": {
                "foreground": "#46e1f4"
            }
        },
        {
            "name": "Quoted Strings",
            "scope": [
                "string.quoted"
            ],
            "settings": {
                "foreground": "#bb86fc"
            }
        },
        {
            "name": "Curly Brackets",
            "scope": [
                "punctuation.definition.block"
            ],
            "settings": {
                "foreground": "#6fd0c8"
            }
        },
        {
            "name": "Normal Brackets",
            "scope": [
                "punctuation.definition.block"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#fd6662"
            }
        },
        {
              "name": "Comment",
              "scope": "comment",
              "settings": {
                "foreground": "#ffe388"
              }
        },
        {
              "name": "Constant",
              "scope": "constant",
              "settings": {
                "foreground": "#9FA8DA"
              }
        },
        {
              "name": "Invalid",
              "scope": "invalid",
              "settings": {
                "foreground": "#f44542"
              }
        },
        {
            "name": "[JAVASCRIPT] - Storage Type Function",
            "scope": "source.js storage.type.function",
            "settings": {
              "foreground": "#fb94ff"
            }
        },
        {
            "name": "[JAVASCRIPT] - Variable Language",
            "scope": "variable.language, entity.name.type.class.js",
            "settings": {
                "foreground": "#fb94ff"
            }
        },
        {
            "name": "[JAVASCRIPT] - Inherited Component",
            "scope": "entity.other.inherited-class.js",
            "settings": {
                "foreground": "#ccc"
            }
        }
            
       
    ]
}